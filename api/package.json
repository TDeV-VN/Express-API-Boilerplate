{
  "name": "gear-up",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf build && mkdir build",
    "build-babel": "babel ./src -d ./build/src",
    "build": "npm run clean && npm run build-babel",
    "production": "npm run build && cross-env BUILD_MODE=production node ./build/src/bin/www",
    "dev": "cross-env BUILD_MODE=dev nodemon --exec babel-node --inspect ./src/bin/www",
    "lint": "eslint src --ext js --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.3",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.2",
    "express": "4.18.2",
    "http-status-codes": "^2.3.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "morgan": "~1.9.1",
    "ms": "^2.1.3",
    "rimraf": "^6.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/node": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.34.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^10.0.0",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10"
  }
}
